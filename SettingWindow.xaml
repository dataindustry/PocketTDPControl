<Window x:Class="PocketTDPControl.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PocketTDPControl"
        mc:Ignorable="d"
        BorderBrush="Black" BorderThickness="1"
        AllowsTransparency="False" WindowStyle="None" WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        Width="200"
        Height="80"
        Title="Setting">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Content="Loopback Exampt" Margin="2" FontSize="10" Click="LoopbackExamptButton_Click" />
        <CheckBox Content="Remap AYANEO 2 Logo Button" Margin="2" FontSize="10" IsChecked="{Binding IsRemapEnabled}" />
        <Button Content="Close" Margin="2" FontSize="10" Click="CloseButton_Click" />
    </StackPanel>
</Window>
